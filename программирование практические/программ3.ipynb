   "source": [
    "import math\n",
    "\n",
    "x = 2.5  # начальное значение\n",
    "eps = 0.001\n",
    "\n",
    "while True:\n",
    "    x_new = 1 / math.sin(x)\n",
    "    if abs(x_new - x) < eps:\n",
    "        break\n",
    "    x = x_new\n",
    "\n",
    "print(f'Корень: {x:.4f}')\n",
    "print(f'Проверка: {x} * sin({x}) = {x * math.sin(x):.4f}')"
   ]
  }
